{
  "name": "rollup-lib-boilerplate",
  "version": "1.0.0",
  "description": "rollup library boilerplate",
  "main": "dist/ai-utils.cjs.js",
  "module": "dist/ai-utils.esm.js",
  "browser": "dist/ai-utils.umd.js",
  "scripts": {
    "build": "rollup -c",
    "test": "npm run test",
    "commit": "git-cz",
    "publish": "npm publish",
    "release": "standard-version",
    "prepare": "husky install",
    "lint": "eslint src",
    "lint:fix": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChineseEngineer/rollup-lib-boilerplate.git"
  },
  "keywords": [
    "rollup",
    "library",
    "boilerplate"
  ],
  "author": "Ethan Tian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChineseEngineer/rollup-lib-boilerplate/issues"
  },
  "homepage": "https://github.com/ChineseEngineer/rollup-lib-boilerplate#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-angular": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
